{
  "name": "JavaScriptCollection",
  "engines": {
    "node": ">=14.20.1"
  },
  "version": "0.0.2",
  "description": "一本有趣的JavaScript合集",
  "scripts": {
    "prepare": "rm -rf .husky &&  husky install && npx husky add .husky/pre-commit \"npm run lintfix && git add .\" && chmod +x .husky/pre-commit",
    "clean": "find . -name \"node_modules\" -type d -exec rm -rf '{}' + && rm -rf docs/.vuepress/dist",
    "dev": "vuepress dev docs",
    "build": "vuepress build docs",
    "build-proxy": "PROXY_DOMAIN=true vuepress build docs",
    "image": "bash scripts/build_image.sh $npm_package_version",
    "faster-image": "bash scripts/build_image.sh $npm_package_version faster",
    "github": "bash scripts/deploy.bash github",
    "lint": "eslint --ext .js,.vue,.ts --ignore-path .gitignore .",
    "lintfix": "eslint --fix --ext .js,.ts,.vue --ignore-path .gitignore ."
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.54.1",
    "@typescript-eslint/parser": "^5.54.1",
    "@vuepress/client": "2.0.0-beta.61",
    "@vuepress/utils": "2.0.0-beta.61",
    "dayjs": "^1.11.7",
    "eslint": "^8.35.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.6.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-vue": "^9.9.0",
    "husky": "^8.0.3",
    "lodash": "^4.17.21",
    "typescript": "^3.9.10",
    "vue": "^3.2.47",
    "vuepress": "2.0.0-beta.61",
    "vuepress-plugin-search-pro": "2.0.0-beta.185",
    "vuepress-theme-hope": "2.0.0-beta.185"
  }
}
